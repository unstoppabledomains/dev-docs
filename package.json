  },
  "dependencies": {
    "@redocly/developer-portal": "^1.1.0-beta.127",
    "polished": "^4.2.2",
    "react": "^17.0.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/node": "^17.0.29",
    "dot-crypto": "https://github.com/unstoppabledomains/dot-crypto.git",
    "ejs": "^3.1.7",
    "markdown-include": "^0.4.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "uns": "https://github.com/unstoppabledomains/uns.git"
  }
}
