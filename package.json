{
  "name": "portal-template",
  "private": true,
  "version": "0.0.1",
  "description": "Redocly portal template repository",
  "license": "MIT",
  "scripts": {
    "start": "redocly-portal develop -p 3000",
    "build": "redocly-portal build --ignore-openapi-errors=true",
